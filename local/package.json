{
  "name": "fc-local-invoke",
  "version": "0.0.23",
  "description": "This is a component demo for Serverless Devs Tool ",
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ServerlessTool/express-alibaba"
  },
  "autoInstall": false,
  "scripts": {
    "preln": "rm -rf ~/.s/components/devsapp.cn/devsapp/fc-local-invoke/dist",
    "ln": "ln -s $(pwd)/dist ~/.s/components/devsapp.cn/devsapp/fc-local-invoke/dist",
    "start": "npm run prebuild && npm run watch",
    "watch": "tsc -w",
    "format": "prettier --write src/",
    "prebuild": "rimraf dist && rimraf node_modules && npm i && npm run format && ex -sc '15i|//@ts-ignore' -cx node_modules/draftlog/index.d.ts",
    "build": "npm run esbuild",
    "esbuild": "ncc build src/index.ts -m -e @serverless-devs/core -o dist",
    "lint": "f2elint scan",
    "fix": "f2elint fix"
  },
  "dependencies": {
    "@alicloud/fc2": "^2.2.2",
    "@serverless-devs/core": "latest",
    "async-lock": "^1.3.0",
    "cheerio": "^1.0.0-rc.9",
    "detect-mocha": "^0.1.0",
    "dev-null": "^0.1.1",
    "dockerode": "^3.3.0",
    "draftlog": "^1.0.12",
    "express": "^4.17.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.1.0",
    "git-ignore-parser": "^0.0.2",
    "http-string-parser": "^0.0.6",
    "httpx": "^2.2.7",
    "ignore": "^5.1.8",
    "ip": "^1.1.5",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "memory-streams": "^0.1.3",
    "nested-object-assign": "^1.0.4",
    "node-watch": "^0.7.1",
    "portfinder": "^1.0.32",
    "promise.prototype.finally": "^3.1.2",
    "raw-body": "^2.4.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "temp-dir": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vercel/ncc": "^0.24.0",
    "eslint": "^7.7.0",
    "f2elint": "^1.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2"
  }
}